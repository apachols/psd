{% extends 'admin/base_site.html' %}
{% load i18n util admin_modify adminmedia %}

{% block title %}
{{command_title}}
{% endblock %}

{% block breadcrumbs %}
{% if not is_popup %}
<div class="breadcrumbs">

<a href="../../admin">{% trans "Home" %}</a>
 &rsaquo;
<a href="{%url edit-event event_name=event.event %}">Edit</a>
 &rsaquo;
<a href="{%url psd.register.views.dashboard.event_manager event_name=event.event, action=main %}">Manage</a>
</div>
{% endif %}
{% endblock %}

{% block object-tools %}
{% if change %}{% if not is_popup %}
  <ul class="object-tools">
    <li><a href="/admin/manage/{{ event_name }}">Manage</a></li>
    <li><a href="history/" class="historylink">"History"</a></li>
  </ul>
{% endif %}{% endif %}
{% endblock %}


{% block header %}
Event Manager for {{ event_name }}
{% endblock %}

{% block content %}

{% if results %} 
<hr>
<h3>Results:</h3>
{{results|safe}}
<hr>
{% endif %}

<h2>{{command_title}} for {{event}}</h2>

<form action="" method="POST">
        <table>
            {{ form.as_table }}
        </table>
        <p><input type="submit" value="{{button_name|default:"Submit"}}"></p>
    </form>

{% endblock %}
